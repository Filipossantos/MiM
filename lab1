.include "m8535def.inc"

LDI R16, low(RAMEND)
OUT SPL, R16
LDI R16, high(RAMEND)
OUT SPH, R16

LDI R16, $FF
OUT DDRA, R16


START:
LDI R16, 0b11001100 
OUT PORTA, R16

RCALL CZAS

LDI R16, 0b00110011 
OUT PORTA, R16

RCALL CZAS

RJMP START

CZAS:

LDI R22, 250
AQQ2:
LDI R21, 255
AQQ:
	DEC R21
	BRNE AQQ
DEC R22
BRNE AQQ2

RET
